{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Mqtt": {
    "Uri" : "<server>",
    "Port" : "<port>",
    "Username" : "<username>",
    "Password" : "<password>",
    "Topic" : "<topic>"
  },
  "Webhook": {
    "SecretHeader": "<X-IotWebhookBridge>",
    "SecretValue": "<secret>"
  },
  "UdpServer": {
    "Port": 15683,
    "IpWhitelist": [ "0.0.0.0/0" ]
  }
}
